{
	"family": "Common-Task-Definition",
	"containerDefinitions": [
		{
			"name": "report-service",
			"image": "553655158632.dkr.ecr.us-west-2.amazonaws.com/roostify/report-service:%BUILD_NUMBER%",
			"cpu": 10,
			"memory": 500,
			"portMappings": [
				{
					"containerPort": 80,
					"hostPort": 8086
				}
			],
			"entryPoint": null,
			"essential": true,
			"environment": [
				{
					"name": "SPRING_PROFILES_ACTIVE",
					"value": "%PROFILE%"
				}
			]
		}
	]
}
